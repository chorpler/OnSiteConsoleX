    "release:win32": "npm run ionic:build && build --win --x64 --publish always",
    "release:darwin": "npm run ionic:build && build --mac --x64 --publish always",
    "releasemac": "npm run release",
    "releasewin": "npm run release",
    "lint": "ionic-app-scripts lint",
    "start": "ionic-app-scripts serve -p 8110 -r 35739 --dev-logger-port 53713 --nobrowser",
    "ionic:build": "ionic-app-scripts build",
    "ionic:buildprod": "ionic-app-scripts build --prod --verbose",
    "ionic:clean": "ionic-app-scripts clean",
    "ionic:serve": "npm run start",
    "ionic:debug": "npm run start --debug"
  },
