  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "chorpler",
        "vPrefixedTagName": true,
        "publishAutoUpdate": true,
        "releaseType": "draft",
        "private": false
      }
    ],
    "appId": "com.sesa.onsiteconsolex",
    "productName": "OnSiteConsoleX",
    "npmRebuild": false,
    "artifactName": "${productName}_${version}_${os}.${ext}",
    "mac": {
      "icon": "resources/icon.icns",
      "category": "public.app-category.business",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "dmg": {
      "background": "resources/backgroundlight.tiff",
      "icon": "resources/icon.icns"
    },
    "win": {
      "icon": "resources/onsitex_icon_multisize.ico",
      "target": [
        "portable",
        "nsis"
      ]
    },
    "nsis": {
      "artifactName": "${productName}_${version}_${os}_installer.${ext}",
      "perMachine": false
    },
    "asar": true,
    "asarUnpack": [
      "node_modules/pouchdb**/*",
      "node_modules/sqlite3**/*"
    ],
    "files": [
      "node_modules/moment/min/**/*",
      "node_modules/jquery/dist/**/*",
      "resources/icon.icns",
      "resources/onsitexconsole.ico",
      "resources/onsitex_icon_multisize.ico",
      "resources/electron*",
      "config/*",
      "www/**/*",
      "electron/electron.js",
      "electron/icon.*"
    ]
  },
